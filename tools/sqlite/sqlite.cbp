<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="sqlite" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release">
				<Option output="bin/Release/sqlite" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Os" />
			<Add option="-fexceptions" />
			<Add option="-DSQLITE_THREADSAFE=0" />
			<Add option="-DSQLITE_DEFAULT_MEMSTATUS=0" />
			<Add option="-DSQLITE_OMIT_DEPRECATED" />
			<Add option="-DSQLITE_DEFAULT_WAL_SYNCHRONOUS=1" />
			<Add option="-DSQLITE_LIKE_DOESNT_MATCH_BLOBS" />
			<Add option="-DSQLITE_MAX_EXPR_DEPTH=0" />
			<Add option="-DSQLITE_OMIT_DECLTYPE" />
			<Add option="-DSQLITE_OMIT_PROGRESS_CALLBACK" />
			<Add option="-DSQLITE_OMIT_SHARED_CACHE" />
			<Add option="-DSQLITE_TEMP_STORE=2" />
			<Add option="-DSQLITE_OMIT_AUTOINIT" />
			<Add option="-DSQLITE_OMIT_COMPILEOPTION_DIAGS" />
		</Compiler>
		<Linker>
			<Add option="-s" />
		</Linker>
		<Unit filename="shell.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="sqlite3.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="sqlite3.h" />
		<Unit filename="sqlite3ext.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
